<IfModule mod_rewrite.c>
Options +FollowSymLinks
RewriteEngine On
RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^taches$ /taches.php
RewriteRule ^classeurs$ /classeurs.php
RewriteRule ^titres /titres.php
RewriteRule ^images /images.php
RewriteRule ^users /users.php

</IfModule>